<View>
    <!-- Control tag for region labels -->
    <TimeSeriesLabels name="label" toName="ts">
        
        
        
        
        
    <Label value="Eating" background="#FFA39E"/><Label value="Smoking" background="#D4380D"/><Label value="Drinking" background="#FFC069"/><Label value="Other" background="#AD8B00"/></TimeSeriesLabels>

    <!-- Object tag for time series data source -->
    <TimeSeries name="ts" valueType="url" value="$timeseriesUrl" sep="," timeColumn="timestamp_formatted" timeFormat="%H:%M:%S.%f" overviewChannels="velocity">

        <Channel column="acc_x" units="m/s^2" displayFormat=",.1f" strokeColor="#1f77b4" legend="Acc X"/>

        <Channel column="acc_y" units="m/s^2" displayFormat=",.1f" strokeColor="#ff7f0e" legend="Acc Y"/>
      
        <Channel column="acc_y" units="m/s^2" displayFormat=",.1f" strokeColor="#ff7f0e" legend="Acc Y"/>
      
    </TimeSeries>
</View>